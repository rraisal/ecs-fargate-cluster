[
  {
    "name": "plan-a-task-prod",
    "image": "<IMAGE1_NAME>",
    "essential": true,
    "portMappings": [{
      "protocol": "tcp",
      "containerPort": "80",
      "hostPort": "80"
    }],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/plan-a-task-prod",
        "awslogs-region": "eu-central-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]